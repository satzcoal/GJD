<h1>Listing order_items</h1>

<table>
  <thead>
    <tr>
      <th>Baseorder</th>
      <th>Item</th>
      <th>Baseprice</th>
      <th>Salprice</th>
      <th>Discount</th>
      <th>Createtime</th>
      <th>Batchid</th>
      <th>Godown</th>
      <th>Sum</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @order_items.each do |order_item| %>
      <tr>
        <td><%= order_item.BaseOrder_id %></td>
        <td><%= order_item.Item_id %></td>
        <td><%= order_item.BasePrice %></td>
        <td><%= order_item.SalPrice %></td>
        <td><%= order_item.Discount %></td>
        <td><%= order_item.CreateTime %></td>
        <td><%= order_item.BatchId %></td>
        <td><%= order_item.GoDown_id %></td>
        <td><%= order_item.Sum %></td>
        <td><%= link_to 'Show', order_item %></td>
        <td><%= link_to 'Edit', edit_order_item_path(order_item) %></td>
        <td><%= link_to 'Destroy', order_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order item', new_order_item_path %>
