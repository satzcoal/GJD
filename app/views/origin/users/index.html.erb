<h1>Listing origin_users</h1>

<% if notice %>
  <p id="notice"><%= notice %></p>
<% end %>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @origin_users.each do |origin_user| %>
      <tr>
        <td><%= origin_user.name %></td>
        <td><%= link_to 'Show', origin_user %></td>
        <td><%= link_to 'Edit', edit_origin_user_path(origin_user) %></td>
        <td><%= link_to 'Destroy', origin_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_origin_user_path %>
