<table class="table table-bordered">
  <tr>
    <th>规格</th>
    <th>&nbsp;</th>
  </tr>
  <tbody>
  <% @sub_categories.each do |sub_category| %>
      <tr>
        <td><%= sub_category.Name %></td>
        <td><%= link_to '编辑', edit_sub_category_path(sub_category) %>
          &nbsp;<%= link_to '删除', sub_category, method: :delete, data: {confirm: '确认删除?'} %></td>
      </tr>
  <% end %>
  </tbody>
</table>


