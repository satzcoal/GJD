<h1>订单详细</h1>
<table>
  <thead>
    <tr>
      <th>订单编号</th>
      <th>物品名称</th>
      <th>生产日期</th>
      <th>保质期</th>
      <th>数量</th>
      <th>实际数量</th>
      <th>生产批次号</th>
      <th>供应商</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sub_orders.each do |sub_order| %>
      <tr>
        <td><%= sub_order.OrderId %></td>
        <td><%= sub_order.ItemNo %></td>
        <td><%= sub_order.CreateTime %></td>
        <td><%= sub_order.ShelfLife %></td>
        <td><%= sub_order.Sum %></td>
        <td><%= sub_order.Sum1 %></td>
        <td><%= sub_order.BatchId %></td>
        <td><%= sub_order.MadeIn %></td>
        <td><%= link_to '查看', sub_order %></td>
        <td><%= link_to '编辑', edit_sub_order_path(sub_order) %></td>
        <td><%= link_to '删除', sub_order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to '新建', new_sub_order_path %>
