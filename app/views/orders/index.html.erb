<h1>列表</h1>

<table>
  <thead>
    <tr>
      <th>订单编号</th>
      <th>订单名称</th>
      <th>创建时间</th>
      <th>创建人</th>
      <th>执行时间</th>
      <th>执行人</th>
      <th>状态</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= order.OrderId %></td>
        <td><%= order.Name %></td>
        <td><%= order.CreateTime %></td>
        <td><%= order.CreateUser %></td>
        <td><%= order.ExecTime %></td>
        <td><%= order.Execer %></td>
        <td><%= order.State %></td>
        <td><%= link_to '查看', order %></td>
        <td><%= link_to '编辑', edit_order_path(order) %></td>
        <td><%= link_to '删除', order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to '新建订单', "/orders/new?type=#{params[:type]}" %>
